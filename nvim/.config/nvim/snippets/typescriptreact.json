{
  "sleep": {
    "prefix": "sleep",
    "body": [
      "await new Promise((r) => setTimeout(r, $1))"
    ]
  },
  "import-styles": {
    "prefix": "import-styles",
    "body": [
      "import * as S from './styles'"
    ]
  },
  "import-mui": {
    "prefix": "import-mui",
    "body": [
      "import { $1 } from '@mui/material'"
    ]
  },
  "import-props": {
    "prefix": "import-props",
    "body": [
      "import { type IProps } from './types'"
    ]
  },
  "disable-next-line": {
    "prefix": "disable-next-line",
    "body": [
      "// eslint-disable-next-line @typescript-eslint/$1"
    ]
  },
  "stack-col": {
    "prefix": "stack-col",
    "body": [
      "<Stack width=\"100%\" spacing={1} alignItems=\"center\">",
      "  $1",
      "</Stack>"
    ]
  },
  "stack-row": {
    "prefix": "stack-row",
    "body": [
      "<Stack width=\"100%\" spacing={1} alignItems=\"center\" direction=\"row\">",
      "  $1",
      "</Stack>"
    ]
  }
}
